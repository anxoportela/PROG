package com.acarballeira.javaFX.ejemplo12.modelo;

import java.util.Objects;

public class Persona {
	
	String nombreString;
	String apellidoString;
	
	public Persona(String nombreString, String apellidoString) {
		super();
		this.nombreString = nombreString;
		this.apellidoString = apellidoString;
	}
	
	public String getNombreString() {
		return nombreString;
	}
	public void setNombreString(String nombreString) {
		this.nombreString = nombreString;
	}
	public String getApellidoString() {
		return apellidoString;
	}
	public void setApellidoString(String apellidoString) {
		this.apellidoString = apellidoString;
	}

	
	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		Persona other = (Persona) obj;
		return Objects.equals(apellidoString, other.apellidoString) && Objects.equals(nombreString, other.nombreString);
	}	

}
